  </org.eclipse.epf.uma:DescriptorDescription>
  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-gsBUX-mDSO-DB1NeXqV58g" name="work_items_list,_tDwlhEl4EeKHJ-K7LFAw7Q" guid="-gsBUX-mDSO-DB1NeXqV58g">
