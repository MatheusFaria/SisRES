  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-OMYhgWh69ksSCSgeitkvFA" name="iteration_plan,_RhMIkkiMEeKveLAxMqzgTg" guid="-OMYhgWh69ksSCSgeitkvFA">
